{
  "name": "calligraphy-classroom",
  "productName": "Calligraphy Classroom Frontend",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "dev": "next dev renderer",
    "dev:desktop": "electron .",
    "build:web": "next build renderer && next export renderer",
    "start": "next start renderer",
    "build:electron": "tsc -p electron-src",
    "build:all": "yarn build:web && yarn build:electron",
    "dist": "npm run build:all && electron-builder --dir",
    "dist:alone": "electron-builder --dir",
    "type-check": "tsc"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "@types/next": "8.0.6",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "electron": "^7.0.1",
    "electron-builder": "^22.1.0",
    "next": "latest",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.25.0",
    "babel-plugin-import": "^1.12.2",
    "less": "3.10.3",
    "less-vars-to-js": "1.3.0",
    "next": "latest",
    "null-loader": "3.0.0",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
