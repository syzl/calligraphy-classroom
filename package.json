{
  "name": "calligraphy-classroom",
  "productName": "Calligraphy Classroom Frontend",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "dev": "next dev renderer",
    "dev:desktop": "electron .",
    "build": "next build renderer && next export renderer",
    "start": "next start renderer",
    "build:electron": "tsc -p electron-src",
    "build:all": "yarn build && yarn build:electron",
    "dist": "npm run build:all && electron-builder --dir",
    "dist:alone": "electron-builder --dir",
    "type-check": "tsc"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash": "^4.14.144",
    "@types/next": "8.0.6",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-infinite-scroller": "^1.2.1",
    "electron": "^7.0.1",
    "electron-builder": "^22.1.0",
    "next": "latest",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.25.0",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "1.5.15",
    "babel-plugin-import": "^1.12.2",
    "cookie": "^0.3.1",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "formik": "^2.0.3",
    "formik-antd": "^1.6.0",
    "framer-motion": "^1.6.15",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.1",
    "https-proxy-agent": "^2.2.1",
    "isomorphic-unfetch": "^2.0.0",
    "js-cookie": "^2.2.1",
    "less": "3.10.3",
    "less-vars-to-js": "1.3.0",
    "lodash": "^4.17.15",
    "next": "latest",
    "next-cookies": "^2.0.1",
    "null-loader": "3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-draggable": "^4.1.0",
    "react-infinite-scroller": "^1.2.4",
    "react-player": "^1.13.0"
  }
}
