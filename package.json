{
  "name": "calligraphy-classroom",
  "private": true,
  "productName": "Calligraphy Classroom Frontend",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "dev": "next dev renderer",
    "dev:desktop": "electron .",
    "export": "next export renderer",
    "build": "next build renderer && next export renderer",
    "start": "next start renderer",
    "build:electron": "tsc -p electron-src",
    "build:all": "yarn build && yarn build:electron",
    "dist": "npm run build:all && electron-builder -w",
    "dist:alone": "electron-builder --dir",
    "type-check": "tsc",
    "tweak": "node bin/tweak.js"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^22.3.2",
    "@types/cookie": "^0.3.3",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash": "^4.14.149",
    "@types/next": "9.0.0",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-svg-inline": "^2.1.1",
    "@types/styled-components": "^5.0.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.26.11",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.3",
    "babel-plugin-import": "^1.13.0",
    "cookie": "^0.4.0",
    "formik": "^2.1.4",
    "formik-antd": "^1.8.1",
    "framer-motion": "^1.8.4",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "https-proxy-agent": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "less": "3.11.1",
    "less-vars-to-js": "1.3.0",
    "lodash": "^4.17.15",
    "next": "^9.2.2",
    "next-cookies": "^2.0.3",
    "null-loader": "3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-player": "^1.15.2",
    "react-rnd": "^10.1.6",
    "react-video-recorder": "^3.17.0",
    "styled-components": "^5.0.1",
    "subscriptions-transport-ws": "^0.9.16"
  }
}
